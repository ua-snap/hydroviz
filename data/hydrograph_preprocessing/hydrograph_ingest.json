{
  "config": {
    "service_url": "https://localhost/rasdaman/ows",
    "tmp_directory": "/tmp/",
    "crs_resolver": "https://localhost/def/",
    "default_null_values": [
      "nan"
    ],
    "mock": false,
    "automated": true,
    "insitu": true
  },
  "input": {
    "coverage_id": "conus_hydro_segments_doy_mmm",
    "paths": [
      "combined_for_rasdaman.nc"
    ]
  },
  "recipe": {
    "name": "general_coverage",
    "options": {
      "tiling": "ALIGNED [0:3, 0:365, 0:1, 0:12, 0:4, 0:0] tile size 761280",
      "wms_import": false,
      "import_order": "ascending",
      "coverage": {
        "crs": "OGC/0/Index1D?axis-label=\"era\"@OGC/0/Index1D?axis-label=\"doy\"@OGC/0/Index1D?axis-label=\"landcover\"@OGC/0/Index1D?axis-label=\"model\"@OGC/0/Index1D?axis-label=\"scenario\"@OGC/0/Index1D?axis-label=\"stream_id\"",
        "metadata": {
          "type": "xml",
          "global": "auto"
        },
        "slicer": {
          "type": "netcdf",
          "pixelIsPoint": true,
          "bands": [
            {
              "name": "doy_min",
              "identifier": "doy_min",
              "nilValue": "nan"
            },
            {
              "name": "doy_mean",
              "identifier": "doy_mean",
              "nilValue": "nan"
            },
            {
              "name": "doy_max",
              "identifier": "doy_max",
              "nilValue": "nan"
            }
          ],
          "axes": {
            "era": {
              "min": "${netcdf:variable:era:min}",
              "max": "${netcdf:variable:era:max}",
              "directPositions": "${netcdf:variable:era}",
              "gridOrder": 0,
              "irregular": true
            },
            "doy": {
              "min": "${netcdf:variable:doy:min}",
              "max": "${netcdf:variable:doy:max}",
              "directPositions": "${netcdf:variable:doy}",
              "gridOrder": 1,
              "irregular": true
            },
            "landcover": {
              "min": "${netcdf:variable:landcover:min}",
              "max": "${netcdf:variable:landcover:max}",
              "directPositions": "${netcdf:variable:landcover}",
              "gridOrder": 2,
              "irregular": true
            },
            "model": {
              "min": "${netcdf:variable:model:min}",
              "max": "${netcdf:variable:model:max}",
              "directPositions": "${netcdf:variable:model}",
              "gridOrder": 3,
              "irregular": true
            },
	        "scenario": {
              "min": "${netcdf:variable:scenario:min}",
              "max": "${netcdf:variable:scenario:max}",
              "directPositions": "${netcdf:variable:scenario}",
              "gridOrder": 4,
              "irregular": true
            },
            "stream_id": {
              "min": "${netcdf:variable:stream_id:min}",
              "max": "${netcdf:variable:stream_id:max}",
              "directPositions": "${netcdf:variable:stream_id}",
              "gridOrder": 5,
              "irregular": true
            }
        }
      }
    }
  }
}
}
